
#setting internal ip for seeds to work - just for testing ...
cseed01 ansible_ssh_host=172.31.27.175
cnode01 ansible_ssh_host=172.31.18.103
cnode02 ansible_ssh_host=172.31.17.158

[cass-seed]
cseed01

[cass-nodes]
cnode01
cnode02

[cass-servers:children]
cass-seed
cass-nodes

[cass-servers:vars]
ansible_ssh_user=ec2-user
ansible_ssh_private_key_file=~/.ssh/mykey.pem
